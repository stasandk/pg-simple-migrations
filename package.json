{
  "name": "pg-simple-migrations",
  "version": "1.0.0",
  "description": "PostgreSQL simple database migrations tool",
  "homepage": "https://github.com/stasandk/pg-simple-migrations#readme",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "pgsm": "bin/pgsm"
  },
  "files": [
    "bin",
    "lib"
  ],
  "author": "Stanislau Karaban",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stasandk/pg-simple-migrations.git"
  },
  "keywords": [
    "pg",
    "npm",
    "package",
    "simple",
    "migrations",
    "database"
  ],
  "scripts": {
    "start": "export $(cat .env.local | xargs) && node lib/index.js",
    "start:local": "export $(cat .env.local | xargs) && ts-node src/index.ts",
    "prepare": "rm -rf ./lib && tsc",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "pg": "^8.3.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/node": "^14.0.25",
    "@types/pg": "^7.14.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
